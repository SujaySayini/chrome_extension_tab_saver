{
    "name": "Tab Saver",
    "version": "1.0.0",
    "description": "Save your Chrome Tabs in case of any random crashes",
    "background": {
        "service_worker": "src/background.js"
     },
     "permissions": [
        "tabs", 
        "background"
      ],
    "manifest_version": 3,
    "author": "Sujay Sayini",
    "action":{
        "default_popup": "index.html",
        "default_title": "Tab Saver"
    }
}